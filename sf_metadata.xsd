			<xs:enumeration value="Report"/>
			<xs:enumeration value="ReportType"/>
			<!--            TODO-->
			<xs:enumeration value="StaticResource"/>
			<!--            TODO-->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="APIAccessLevel">
		<xs:restriction>
			<xs:enumeration value="Unrestricted">
				<!-- TODO docs-->
			</xs:enumeration>
			<xs:enumeration value="Restricted">
				<!-- TODO docs-->
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="LanguageEnum" final="restriction">
		<xs:restriction>
			<xs:enumeration value="en_US"/>
			<!--      TODO more-->
		</xs:restriction>
	</xs:simpleType>
</xs:schema>